{
  "name": "yaml-cli",
  "version": "1.0.1",
  "description": "A simple CLI for querying and updating YAML files",
  "bin": {
    "yaml": "bin/yaml"
  },
  "scripts": {
    "test": "coffee -o lib/ -c src/*.*coffee && test/test.sh",
    "prepublish": "coffee -o lib/ -c src/*.*coffee",
    "tag": "(node_modules/.bin/json -f package.json version | xargs -I version git tag -am version version) && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandastrike/yaml-cli.git"
  },
  "keywords": [
    "yaml",
    "cli"
  ],
  "author": "Dan Yoder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pandastrike/yaml-cli/issues"
  },
  "homepage": "https://github.com/pandastrike/yaml-cli#readme",
  "dependencies": {
    "fairmont": "^1.0.0-beta-40",
    "js-yaml": "^3.5.3"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "json": "^9.0.3"
  }
}
